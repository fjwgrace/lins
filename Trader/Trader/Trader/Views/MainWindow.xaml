<Window x:Class="Trader.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:md="http://materialdesigninxaml.net/winfx/xaml/themes"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Trader.Views"
        WindowStartupLocation="CenterScreen"
        mc:Ignorable="d"
        Title="" Height="550" Width="1600" Style="{StaticResource MaterialDesignWindow}" >
    <Grid>
        <TabControl TabStripPlacement="Left" Style="{StaticResource MaterialDesignTabControl}"  >
            <TabItem Header="持仓">
                <local:PositionView/>
            </TabItem>
            <TabItem Header="委托">
                <local:OrderView/>
            </TabItem>
            <TabItem Header="成交">
                <local:DealView/>
            </TabItem>
        </TabControl>
    </Grid>
</Window>
